from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
import time

# Path to ChromeDriver
chromedriver_path = '/path/to/chromedriver'

# Initialize the Chrome WebDriver
driver = webdriver.Chrome(executable_path=chromedriver_path)

# Open the SharePoint login page
driver.get('https://your-sharepoint-site-url')

# Allow time for the page to load
time.sleep(2)

# Locate and fill the username field (assuming Office 365 login page)
username_field = driver.find_element(By.ID, 'i0116')
username_field.send_keys('your-email@example.com')
username_field.send_keys(Keys.RETURN)

# Allow time for the next page to load
time.sleep(2)

# Locate and fill the password field
password_field = driver.find_element(By.ID, 'i0118')
password_field.send_keys('your-password')
password_field.send_keys(Keys.RETURN)

# Allow time for authentication (handle any MFA if necessary)
time.sleep(5)

# Navigate to the specific SharePoint .aspx page
driver.get('https://your-sharepoint-site-url/sites/YourSite/Pages/YourPage.aspx')

# Allow time for the page to load
time.sleep(2)

# Extract the data you need (example: getting the page content)
page_content = driver.find_element(By.TAG_NAME, 'body').text
print(page_content)

# Close the browser
driver.quit()
